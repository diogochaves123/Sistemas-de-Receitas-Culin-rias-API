{
  "name": "sistema-receitas-culinarias",
  "version": "1.0.0",
  "description": "API RESTful completa para Sistema de Receitas Culin√°rias",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "sequelize-cli db:migrate",
